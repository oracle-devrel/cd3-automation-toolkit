#Cipher Suite
resource "oci_load_balancer_ssl_cipher_suite" "{{ lbr_tf_name }}_{{ cipher_tf_name }}" {
    #Required
    ciphers = [{{ ciphers }}]
    name = "{{ cipher_suite_name }}"

    #Optional
    load_balancer_id = oci_load_balancer_load_balancer.{{ lbr_tf_name }}.id

    lifecycle {
        ignore_changes = [ciphers]
    }
}